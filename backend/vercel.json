{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "MONGODB_URL": "@mongodb_url",
    "DATABASE_NAME": "mental_wellness",
    "FRONTEND_URL": "https://tejj.me"
  },
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
}